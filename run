#!/bin/bash

echo "----------Install Scipt----------"

mkdir install-script-files

cd install-script-files

wget -q "https://raw.githubusercontent.com/cezarmathe/install-script/developement/install.sh" && chmod +x install.sh &&

wget -q "https://raw.githubusercontent.com/cezarmathe/install-script/developement/varsetup.sh" && chmod +x varsetup.sh &&

wget -q "https://raw.githubusercontent.com/cezarmathe/install-script/developement/pkg-install.sh" && chmod +x pkg-install.sh &&

./varsetup.sh &&

./install.sh
